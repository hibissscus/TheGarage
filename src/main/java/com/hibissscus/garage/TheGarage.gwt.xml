<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://www.gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='theGarage'>
    <!-- Inherit the core Google Web Toolkit stuff. -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.i18n.I18N'/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>

    <set-property name="gwt.logging.consoleHandler" value="DISABLED"/>
    <set-property name="gwt.logging.developmentModeHandler" value="ENABLED"/>

    <inherits name="com.google.gwt.resources.Resources"/>

    <!-- Gin -->
    <inherits name="com.google.gwt.inject.Inject"/>

    <!-- RestyGWT -->
    <inherits name="org.fusesource.restygwt.RestyGWT"/>

    <!-- Inherit from guava's collections module -->
    <inherits name="com.google.common.collect.Collect"/>
    <inherits name="org.vectomatic.libgwtsvg"/>

    <!-- English language, independent of country -->
    <extend-property name="locale" values="en"/>
    <set-property-fallback name="locale" value="en"/>

    <!-- Other module inherits -->

    <!-- Specify the paths for translatable code -->
    <source path='client'/>
    <source path='shared'/>

    <!-- Specify the app entry point class. -->
    <entry-point class='com.hibissscus.garage.client.TheGarageEntryPoint'/>
</module>
