<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="com.hibissscus.garage.client.view.resource.ApplicationResources"/>

    <g:HTMLPanel>
        <table cellpadding="0" cellspacing="0" border="0">
            <tr>
                <td>
                    <g:InlineLabel ui:field="textLabel"/>
                </td>
                <td>
                    <g:TextBox text="0" ui:field="textBox" visibleLength="1" maxLength="1"/>
                </td>
                <td>
                    <table cellspacing="0">
                        <tr>
                            <td>
                                <g:Image ui:field="arrowUpImage" resource="{res.arrowUpImage}"/>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <g:Image ui:field="arrowDownImage" resource="{res.arrowDownImage}"/>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>

</ui:UiBinder>
